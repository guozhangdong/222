    "crypto": "^1.0.1",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "dotenv": "^16.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-koa": "^1.0.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.1",
    "qrcode": "^1.5.3",
    "node-cache": "^5.1.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5"
