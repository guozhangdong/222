  },
  "keywords": ["koa", "sequelize", "wechat", "miniprogram", "payment"],
  "author": "邻·生活团队",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.14.2",
    "koa-router": "^12.0.0",
    "koa-bodyparser": "^4.4.1",
    "koa-cors": "^0.0.16",
    "koa-static": "^5.0.0",
    "koa-ratelimit": "^5.1.0",
    "koa-xml-body": "^1.0.0",
    "koa-helmet": "^7.0.2",
    "koa-compress": "^5.1.1",
    "sequelize": "^6.32.1",
    "mysql2": "^3.6.0",
    "redis": "^4.6.7",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
